@model IEnumerable<Chat>
    <div class="chat-body">
        @foreach (var chat in Model)
        {
            <button type="button" class="btn btn-primary mt-3" id="room-button">
                <a asp-controller="Home" asp-action="JoinRoom" asp-route-id="@chat.Id">
                    @chat.Name
                </a>
            </button>
        }
    </div>